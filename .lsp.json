{
  "swift": {
    "command": "sourcekit-lsp",
    "extensionToLanguage": {
      ".swift": "swift"
    }
  },
  "kotlin": {
    "command": "kotlin-language-server",
    "extensionToLanguage": {
      ".kt": "kotlin",
      ".kts": "kotlin"
    }
  },
  "typescript": {
    "command": "typescript-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".ts": "typescript",
      ".tsx": "typescriptreact",
      ".js": "javascript",
      ".jsx": "javascriptreact",
      ".mjs": "javascript",
      ".cjs": "javascript"
    }
  }
}
